[access]
	inheritFrom = All-Projects
[project]
	description = Management repo for Jenkins Job Builder, builder scripts and management related to the CI configuration.
[access "refs/*"]
	viewDrafts = group Anonymous Users
	read = group GitHub Replication
	owner = group saml/onap-gerrit-ci-management-committers
[access "refs/heads/*"]
	submit = deny group Release Engineering Team
	label-Code-Review = deny -2..+2 group Release Engineering Team
[label "Code-Review"]
	function = MaxWithBlock
	defaultValue = 0
	value = -2 This shall not be merged
	value = -1 I would prefer this is not merged as is
	value = 0 No score
	value = +1 Looks good to me, but someone else must approve
	value = +2 Looks good to me, approved
	ignoreSelfApproval = true
	copyCondition = changekind:NO_CHANGE OR changekind:TRIVIAL_REBASE OR is:MIN
[submit-requirement "Verified"]
	description = A maximum 'Verified' vote from 'ONAP Required GHA' and 'ONAP GitHub Actions' is required for the 'Verified' label. A minimum vote is blocking.
	submittableIf = label:Verified=MAX,user=onap.required.gha AND label:Verified=MAX,user=ecomp-jobbuilder AND -label:Verified=MIN
	canOverrideInChildProjects = true
